    "version": "0.2.0",
    "configurations": [
        {
            "name": "Testing - Run Unit Tests",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}",
            "args": ["./..."],
            "logOutput": "dap",
            "showLog": true,
        },
        {
            "name": "Launch - No Params",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "env": {"AWS_REGION": "us-east-1"}
        },
        {
            "name": "Launch - Help",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "env": {"AWS_REGION": "us-east-1"},
            "args": ["--help"]
        },
        {
            "name": "Launch - IP Param - Test IP - ALB",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "env": {"AWS_REGION": "us-east-1"},
            "args": ["--ipaddr=52.4.175.237", "--verbose"]
        },
        {
            "name": "Launch - IP Param - Test IP - ALB - Organizations",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "env": {"AWS_REGION": "us-east-1"},
            "args": ["--ipaddr=52.4.175.237", "--org-search", "--org-search-role-name=ip2cr-xaccount-connector", "--verbose"]
        },
        {
            "name": "Launch - IP Param - Test IP - ALB - Silent Output",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "env": {"AWS_REGION": "us-east-1"},
            "args": ["--silent", "--ipaddr=52.4.175.237", "--verbose"]
        },
        {
            "name": "Launch - IP Param - Test IP - ALB - JSON Output",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "env": {"AWS_REGION": "us-east-1"},
            "args": ["--json", "--ipaddr=52.4.175.237", "--verbose"]
        },
        {
            "name": "Launch - IP Param - Test IP - Cloudfront",
            "type": "go",
            "request": "launch",
            "mode": "debug",
