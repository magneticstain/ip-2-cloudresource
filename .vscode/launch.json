    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch - No Params",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "env": {"AWS_REGION": "us-east-1"}
        },
        {
            "name": "Launch - Help",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "env": {"AWS_REGION": "us-east-1"},
            "args": ["--help"]
        },
        {
            "name": "Launch - IP Param - Test IP",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "env": {"AWS_REGION": "us-east-1"},
            "args": ["--ipaddr=18.161.22.61"]
        },
        {
            "name": "Launch - IP Param - Test Invalid IP",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "main.go",
            "env": {"AWS_REGION": "us-east-1"},
            "args": ["--ipaddr=1.1.1.1"]
        }
    ]
}